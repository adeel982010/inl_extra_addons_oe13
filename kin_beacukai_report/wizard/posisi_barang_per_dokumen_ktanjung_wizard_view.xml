<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_excel_posisi_barang_per_dokumen_ktanjung_wiz" model="ir.ui.view">
        <field name="name">Posisi Barang Per Dokumen Kuala Tanjung</field>
        <field name="model">posisi.barang.per.dokumen.ktanjung.export.summary</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Excel Export">
               <group cospan="2" col="2">
                    <field name="name" invisible="1"/>
                    <h4>Click On Link To Download Xls File: &#160;<br/><br/><field name="file" filename="name" nolabel="1"/></h4>
                </group>
                <footer>
                    <button special="cancel" string="Cancel" colspan="2" />
                </footer>
            </form>
        </field>
    </record>

    <record id="view_export_posisi_barang_per_dokumen_ktanjung_wiz" model="ir.ui.view">
        <field name="name">Posisi Barang per Dokumen Kuala Tanjung Export Wizard</field>
        <field name="model">posisi.barang.per.dokumen.ktanjung.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="View Posisi Barang per Dokumen Export">
                <group col="4">
                    <field name="date_start" required="1" />
                    <field name="date_end" required="1" />
                </group>
                <footer>
                    <button name="print_inventory_preview_report" string="Preview" type="object" colspan="2" class="oe_highlight" icon="fa-search-plus"/> 
                    <button name="print_inventory_export_report" string="Export" type="object" colspan="2" class="oe_highlight" icon="fa-file-excel-o"/> 
                    <button special="cancel" string="Cancel" colspan="2" />
                </footer>
            </form>
        </field>
    </record>

</odoo>